  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "prettier-tslint": "^0.4.0",
    "react-addons-test-utils": "^15.6.2",
    "styled-components": "^3.4.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
